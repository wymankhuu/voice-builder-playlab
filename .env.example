# ElevenLabs API Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=default_voice_id

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Whisper Speech-to-Text Configuration (optional)
# Enable Whisper for more accurate speech-to-text (uses OpenAI API key above)
# When enabled, audio chunks are sent to Whisper API for transcription
# Cost: $0.006 per minute (~$0.02 per interview)
# When disabled, uses browser-based Web Speech API (free, instant, Chrome/Edge only)
ENABLE_WHISPER=false

# Server Configuration
PORT=3000
NODE_ENV=development

# Client Configuration (Vite)
VITE_API_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3000
